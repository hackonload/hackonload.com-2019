{{#if home}}
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            let devfolioOptions = {
                buttonSelector: '#devfolio-apply-now',
                key: 'onload',
            };

            let script = document.createElement('script');
            script.src = "https://apply.devfolio.co";
            document.head.append(script);
            script.onload = function () {
                new Devfolio(devfolioOptions);
            };
            script.onerror = function () {
                document.querySelector(devfolioOptions.buttonSelector).addEventListener('click', function () {
                    window.location.href = 'https://devfolio.co/external-apply/' + devfolioOptions.key;
                });
            }
        });

        const switchToMinus = (id) => {
            const selector = "div#" + id + ">h5.mb-0>button>i.plus-minus";
            $(selector).removeClass("fa-plus-circle").addClass("fa-minus-circle");
        };

        const switchToPlus = (id) => {
            const selector = "div#" + id + ">h5.mb-0>button>i.plus-minus";
            $(selector).removeClass("fa-minus-circle").addClass("fa-plus-circle");
        };
            {{#each faqs}}
            $("#{{collapse}}").on("show.bs.collapse", () => {
                switchToMinus("{{id}}");
            });
            $("#{{collapse}}").on("hidden.bs.collapse", () => {
                switchToPlus("{{id}}");
            });
            {{/each}}
    </script>
{{/if}}
