<!----FAQ Section---->

<section class="faq section pt-0" id="#faqs">
    <div class="container padding-30">
        <div class="row">
            <div class="col-sm-8 m-auto page-title">
                <h2>Frequently Asked Questions</h2>
                <p>Some text....</p>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-10 m-auto">
                <div class="block shadow">
                    <div class="faq-item">
                        {{#each faqs}}
                            <div class="item">
                                <div class="item-link">
                                    <a data-toggle="collapse" href="#{{id}}">
                                        <i class="fas fa-chevron-down"></i> {{question}}
                                    </a>
                                </div>
                                <div id="{{id}}" class="collapse accordion-block">
                                    <p>
                                        {{answer}}
                                    </p>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container padding-30 text-center">
        <h4>Have any more Questions?</h4>
        <div class="row">
            <div class="col-6 text-center">
                <h5>
                    <i class="fas fa-envelope"></i> <a href="mailto:contact@hackonload.com">contact@hackonload.com</a>
                </h5>
            </div>
            <div class="col-6"><h5>
                <i class="fab fa-twitter"></i> <a href="mailto:contact@hackonload.com">Tweet to us</a>
            </h5></div>
        </div>
    </div>
</section>

<!--====  End of FAQ Section  ====-->

